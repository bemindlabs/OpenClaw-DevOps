{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "version": "1.0.0",
  "lastUpdated": "2026-02-14",
  "description": "Centralized port registry for OpenClaw DevOps platform. All services use ports in the 32100-32199 range.",
  "portRange": {
    "start": 32100,
    "end": 32199,
    "total": 100
  },
  "categories": {
    "core": {
      "range": "32100-32119",
      "description": "Core application services (Nginx, Landing, Assistant, Gateway, Portainer)"
    },
    "databases": {
      "range": "32120-32139",
      "description": "Database services (MongoDB, PostgreSQL, Redis)"
    },
    "messaging": {
      "range": "32140-32159",
      "description": "Messaging and workflow services (Zookeeper, Kafka, n8n)"
    },
    "monitoring": {
      "range": "32160-32179",
      "description": "Monitoring and observability (Prometheus, Grafana, Exporters)"
    },
    "optional": {
      "range": "32180-32189",
      "description": "Optional AI services (Ollama, LM Studio)"
    },
    "reserved": {
      "range": "32190-32199",
      "description": "Reserved for future expansion"
    }
  },
  "services": {
    "nginx-http": {
      "name": "Nginx HTTP",
      "category": "core",
      "port": 32100,
      "oldPort": 80,
      "envVar": "NGINX_HTTP_PORT",
      "protocol": "http",
      "healthCheck": "http://localhost:32100/health",
      "description": "Main reverse proxy HTTP endpoint",
      "configFiles": [
        "docker-compose.yml",
        "docker-compose.full.yml",
        "nginx/nginx.conf"
      ],
      "dependencies": [],
      "externalFacing": true
    },
    "nginx-https": {
      "name": "Nginx HTTPS",
      "category": "core",
      "port": 32101,
