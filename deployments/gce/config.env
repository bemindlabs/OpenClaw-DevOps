# GCE Deployment Configuration
# Project: your-gcp-project-id
# Instance: openclaw-server
# Zone: us-central1-a

# GCP Settings
GCP_PROJECT=your-gcp-project-id
GCP_ZONE=us-central1-a
GCP_REGION=us-central1
GCP_INSTANCE=openclaw-server

# Server Settings
SERVER_USER=lps
SERVER_DIR=/home/lps/server

# Application Settings
LANDING_PORT=3000
GATEWAY_PORT=18789
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Domain Settings
LANDING_DOMAIN=your-domain.com
GATEWAY_DOMAIN=openclaw.your-domain.com

# Docker Settings
LANDING_IMAGE=openclaw-landing:latest
NGINX_IMAGE=nginx:alpine

# Environment
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
